{
  "name": "next-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "next": "^6.0.4-canary.3",
    "next-redux-wrapper": "^2.0.0-beta.6",
    "next-styled-css": "^0.0.0-beta.5",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-handy": "^0.0.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-flow": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-2": "^7.0.0-beta.44",
    "@babel/register": "^7.0.0-beta.49",
    "autoprefixer": "^8.3.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^7.0.0-beta.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-postcss": "^7.0.1",
    "gulp-stylus": "^2.7.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.3",
    "postcss-filters": "^0.0.2",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.4",
    "postcss-selector-not": "^3.0.1",
    "styled-css-loader": "^0.0.0-beta.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "scripts": {
    "dev": "nodemon -q -w src/server --exec \"npm run compile:server && node dist/index.js\"",
    "dev:css": "rimraf public/_dist && gulp css/watch",
    "dev:client": "node dist/index.js",
    "dev:server": "DISABLE_NEXT=true nodemon -q -w src/server --exec \"npm run compile:server && node dist/index.js\"",
    "start": "NODE_ENV=production node dist/index.js",
    "build": "NODE_ENV=production gulp && npm run compile:server && next build src",
    "test": "$(npm bin)/mocha --require @babel/register",
    "test:watch": "$(npm bin)/mocha --watch --require @babel/register",
    "compile:css": "gulp css",
    "compile:server": "rm -rf dist && $(npm bin)/babel --no-babelrc --config-file=./.babelrc.server.json src/server -d dist"
  }
}
